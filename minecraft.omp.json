{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "alignment": "left",
      "segments": [
        {
          "foreground": "#8B5A2B",
          "style": "plain",
          "template": "{{ .UserName }} ",
          "type": "session"
        },
        {
          "foreground": "#808080",
          "style": "plain",
          "template": "in ",
          "type": "text"
        },
        {
          "foreground": "#55FF55",
          "options": {
            "style": "full"
          },
          "style": "plain",
          "template": "{{ .Path }} ",
          "type": "path"
        },
        {
          "foreground": "#aaaaaa",
          "options": {
            "branch_ahead_icon": "\u2191",
            "branch_behind_icon": "\u2193",
            "branch_gone_icon": "\u2260",
            "branch_icon": "",
            "branch_identical_icon": "\u2261",
            "fetch_status": true
          },
          "style": "plain",
          "template": "<#ffffff>on</> {{ .HEAD }}{{ if .Staging.Changed }}<#55FF55> \u25a0 {{ .Staging.String }}</>{{ end }}{{ if .Working.Changed }}<#FF5555> \u25a0 {{ .Working.String }}</>{{ end }} ",
          "type": "git"
        },
        {
          "foreground": "#55FFFF",
          "style": "plain",
          "template": " ‚õè ",
          "type": "text"
        }
      ],
      "type": "prompt"
    }
  ],
  "version": 4
}
